{
  "name": "blue-witness",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "repository": "github:Lambda-School-Labs/human-rights-first-fe-a",
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@okta/okta-auth-js": "^4.8.0",
    "@okta/okta-react": "^4.1.0",
    "@okta/okta-signin-widget": "^4.1.2",
    "@reduxjs/toolkit": "^1.5.0",
    "antd": "^4.15.5",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "craco": "0.0.3",
    "craco-less": "^1.17.0",
    "lodash": "^4.17.21",
    "luxon": "^1.25.0",
    "moment": "^2.29.1",
    "nanoid": "^3.1.22",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^16.13.1",
    "react-map-gl": "^5.2.10",
    "react-moment": "^1.0.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-twitter-embed": "^3.0.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "scss": "^0.2.4",
    "styled-components": "^5.2.1",
    "supercluster": "^7.1.0",
    "vertical-timeline-component-for-react": "^1.0.7"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "coverage": "npm test -- --coverage --watchAll=false",
    "lint": "eslint 'src/**/*.{js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss}'",
    "storybook": "start-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/stories/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^@okta/okta-auth-js$": "<rootDir>/node_modules/@okta/okta-auth-js/dist/okta-auth-js.umd.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^6.2.9",
    "@storybook/storybook-deployer": "^2.8.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^7.2.1",
    "babel-loader": "8.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-prop-type-error": "^1.1.0",
    "kleur": "^4.1.1",
    "lint-staged": "^10.2.11",
    "prettier-eslint-cli": "^5.0.1",
    "prompts": "^2.3.2",
    "sass": "^1.32.5",
    "webpack": "4.44.2",
    "yaml": "^1.10.2"
  }
}
